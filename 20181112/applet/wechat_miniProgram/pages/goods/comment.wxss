#goTop,.goTop {
	bottom: 100rpx;
	right: 0;
	position: fixed;
	z-index: 450
}

#goTop li,.goTop li {
	padding-top: 16rpx;
	padding-bottom: 8rpx;
	text-align: center;
	padding-right: 4rpx
}

#goTop li .icon-cart,#goTop li span,.goTop li .icon-cart,.goTop li span {
	height: 80rpx;
	width: 80rpx;
	display: inline-block
}

#goTop li .icon-cart,.goTop li .icon-cart {
	-webkit-border-radius: 80rpx;
	border-radius: 80rpx;
	position: relative
}

#goTop li .icon-cart span,.goTop li .icon-cart .span {
	color: #592d4a;
	position: absolute;
	min-width: 1em;
	height: 1em;
	right: -3rpx;
	background-color: #ffe814;
	padding: 3rpx;
	-webkit-border-radius: 1em;
	border-radius: 1em
}

#goTop li .icon-gotop,.goTop li .icon-gotop {
	background: url(http://s16.mogucdn.com/p1/161104/idid_ifrwmmrrmyywinjqmmzdambqhayde_90x90.png) no-repeat center center;
	background-size: 100%;
	height: 80rpx;
	width: 80rpx;
	display: inline-block
}

#goTop li .icon-gohome,#goTop li .icon-gotop,.goTop li .icon-gohome,.goTop li .icon-gotop {
	-webkit-border-radius: 80rpx;
	border-radius: 80rpx
}

#goTop li .icon-collect,.goTop li .icon-collect {
	background: url(http://s16.mogucdn.com/p1/161114/idid_ifrwkmzrgbstmojtmmzdambqhayde_135x135.png) no-repeat center center;
	background-size: 100%
}

#goTop li .icon-no-collect,.goTop li .icon-no-collect {
	background: url(http://s16.mogucdn.com/p1/161114/idid_ifrgiojrmm4tmojtmmzdambqmeyde_135x135.png) no-repeat center center;
	background-size: 100%
}

.exposure-num {
	position: relative;
	border-radius: 50%;
	background-color: rgba(0,0,0,.8);
	color: #fff;
	font-size: 20rpx
}

.exposure-num:after,.exposure-num:before {
	position: absolute;
	left: 0;
	width: 100%;
	text-align: center
}

.exposure-num:before {
	top: 17%;
	content: attr(data-cur)
}

.exposure-num:after {
	content: attr(data-total);
	bottom: 14%
}

.exposure-num .bar {
	position: absolute;
	width: 70%;
	height: 1px;
	background: #fff;
	left: 50%;
	top: 50%;
	-webkit-transform: translate3d(-50%,-50%,0);
	-moz-transform: translate3d(-50%,-50%,0);
	-ms-transform: translate3d(-50%,-50%,0);
	-o-transform: translate3d(-50%,-50%,0);
	transform: translate3d(-50%,-50%,0)
}

#goTop .show-exposure .exposure-num {
	display: inline-block
}

#goTop .exposure-num,#goTop .show-exposure .icon-gotop {
	display: none
}

