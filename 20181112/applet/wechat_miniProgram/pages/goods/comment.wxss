.pullUp {
	text-align: center;
	line-height: 40px;
	font-size: 14px;
	color: #888;
	position: relative;
	margin-left: auto;
	margin-right: auto;
	-webkit-transition: -webkit-transition .25s ease-out 0;
	width: 40px;
	height: 40px
}

.pullUp[status=loading] {
	background-position: 0 100%;
	transform: rotate(0deg) translateZ(0);
	-webkit-transform: rotate(0deg) translateZ(0);
	-webkit-transition-duration: 0ms;
	-webkit-animation-name: loading;
	-webkit-animation-duration: 2s;
	-webkit-animation-iteration-count: infinite;
	-webkit-animation-timing-function: linear
}

@-webkit-keyframes loading {
	0% {
		transform: rotate(0deg) translateZ(0);
		-webkit-transform: rotate(0deg) translateZ(0)
	}

	to {
		transform: rotate(360deg) translateZ(0);
		-webkit-transform: rotate(360deg) translateZ(0)
	}
}

.pullUp[status=stop],.pullUp[status=tap] {
	-webkit-animation-name: none;
	background: 0
}

.pullUp:before {
	white-space: nowrap;
	margin-left: -10px
}

.pullUp[status=tap]:before {
	content: "上拉看更多..."
}

.pullUp[status=stop]:before {
	content: "木有了..."
}

.imageSlide {
	position: fixed;
	overflow: hidden;
	font-size: 0;
	width: 640rpx;
	height: 800rpx;
	top: 50%;
	left: 0;
	margin-top: -400rpx
}

.imageSlide ul {
	position: absolute;
	overflow-y: hidden
}

.imageSlide li {
	position: relative;
	float: left;
	display: inline;
	width: 640rpx;
	height: 800rpx
}

.imageSlide .list_bannar,.mask {
	background-size: cover;
	background-repeat: no-repeat;
	background-position: center
}

.imageSlide .list_bannar {
	height: 800rpx;
	width: 100%;
	display: inline-block
}

.swipe_num {
	position: absolute;
	right: 15px;
	bottom: 15px;
	width: 40px;
	height: 16px;
	background: rgba(0,0,0,.4);
	color: #fff;
	font-size: 20rpx;
	border-radius: 10px;
	text-align: center
}

.swipe_num em {
	font-style: normal
}

.mask {
	display: none;
	background-color: rgba(0,0,0,.6);
	position: fixed;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	z-index: 700
}

.animated {
	-webkit-animation-duration: .8s;
	animation-duration: .8s;
	-webkit-animation-fill-mode: both;
	animation-fill-mode: both
}

@-webkit-keyframes zoomIn {
	0% {
		opacity: 0;
		-webkit-transform: scale3d(.3,.3,.3);
		transform: scale3d(.3,.3,.3)
	}

	50% {
		opacity: 1
	}
}

@keyframes zoomIn {
	0% {
		opacity: 0;
		-webkit-transform: scale3d(.3,.3,.3);
		transform: scale3d(.3,.3,.3)
	}

	50% {
		opacity: 1
	}
}

.zoomIn {
	-webkit-animation-name: zoomIn;
	animation-name: zoomIn
}
