.nav{
  white-space: nowrap;
  background: #fff;
  line-height: 1rem;
  padding: 0.5rem 0;
  font-size: 0.75rem;
  position:fixed;
  width:100%;
  top:0;
  z-index:999;
}
.hide{display:none;}
.nav_item{
  display: inline-block;
  padding: 0 10px;
  margin: 0 5px;
  font-size: 28rpx;
  line-height: 1.4rem;
}
.nav .cur{
  background: #ff5777;
  border-radius: 10px;
  color: #fff;
}
.banner{
  background: #fff;
  margin-top:2rem;
}
.swiper {
  height: 8.8rem;
  width: 100%;
}
.swiper image {
  height: 100%;
  width: 100%;
}
.ad{
  overflow: hidden;
  padding: 8px;
