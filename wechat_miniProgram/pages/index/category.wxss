  background: #fff;
}
.ad .ad_nav{
  float: left;
  width: 49%;
  height: 5.5rem;
}
.ad .ad_l{
  margin-right: 1%;
}
.ad .ad_r{
  margin-left: 1%;
}
.ad .ad_nav image{
  width: 100%;
  height: 100%;
}
.goods{
  margin-top:88rpx; 
  margin-bottom: 50px;
  overflow: hidden;
}


#quick-entrance-new-2 {
    width: 100%;
    display: block;
    position: relative;
    background-color: #fff;
    padding: 9rpx 9rpx 0rpx;
}
.qe-wrapper-2 {
    position: relative;
    width: 100%;
    background-color: #fff;
    padding: 7px 0 4px;
    white-space: nowrap;
    -webkit-overflow-scrolling: touch;
    overflow: hidden;
}
.qe-wrapper-2 .li {
    width: 20%;
    float: left;
    display: block;
    margin-bottom: 10rpx;
}
.qe-wrapper-2 .li:nth-child(1) {
    margin-left: 0;
}
.qe-wrapper-2 .li .quick-entrance-cube {
    display: block;
    padding: 0;
    width: 100%;
    height: inherit;
    text-align: center;
    color: #fff;
    position: relative;
}
.qe-wrapper-2 .li .quick-entrance-cube image {
    width: 78rpx;
    height: 78rpx;
}
.qe-wrapper-2 .li .quick-entrance-cube .cube-text {
    position: relative;
    width: 100%;
    height: 32rpx;
    line-height: 32rpx;
    color: #777;
    margin-top: 5rpx;
    font-size: 24rpx;
}

.span{display: inline;}

/*voucher begin */
.promotion {
    padding: 20rpx 7rpx 0rpx 7rpx;
    background-color:#fff;
}
.head {
    font-size: 24rpx;
    color: #999;
    margin-bottom: 16rpx;
}
.slider {
    position: relative;
    overflow: hidden;
}
.slider-wraper {
    position: relative;
    width: 100%;
    height: 100%;
    z-index: 1;
    display: -webkit-box;
    display: -webkit-flex;
    display: -moz-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-transform-style: preserve-3d;
    transform-style: preserve-3d;
    -webkit-transition-property: -webkit-transform;
    transition-property: -webkit-transform;
    transition-property: transform;
    transition-property: transform,-webkit-transform;
    -webkit-transform: translateZ(0);
    transform: translateZ(0);
}
