.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.weui-loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}
.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}

.pin_tips_container {
    opacity: 0;
    position: fixed;
    top: 120rpx;
    left: 20rpx;
    min-width: 300rpx;
    height: 60rpx;
    -webkit-border-radius: 10rpx;
    border-radius: 10rpx;
    overflow: hidden;
    color: #fff;
    background: rgba(0,0,0,.5);
    z-index: 150;
    transition: opacity 1s;
    -webkit-transition: opacity 1s;
}
.pin_tips_fadeOut {
    opacity: 0;
}
.fl {
    float: left;
}
.pin_tips_user_pic {
    float: left;
    height: 56rpx;
    width: 56rpx;
    -webkit-border-radius: 10rpx;
    border-radius: 10rpx;
    overflow: hidden;
    margin-left: 2rpx;
    margin-top: 2rpx;
}
.pin_tips_container:after {
    clear: both;
    content: ".";
    display: block;
    visibility: hidden;
    height: 0;
}
.pin_tips_info {
    float: right;
    max-width: 600rpx;
    line-height:60rpx;
    font-size: 24rpx;
    margin: 0 20rpx;
}
.pin_tips_info_name {
    max-width: 310rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}
.pin_tips_fadeIn {
	opacity: 1
}


