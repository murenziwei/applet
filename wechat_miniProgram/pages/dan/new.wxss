/**index.wxss**/
.nav{
  white-space: nowrap;
  background: #fff;
  line-height: 1rem;
  padding: 0.5rem 0;
  font-size: 0.75rem;
  position:fixed;
  width:100%;
  top:0;
  z-index:999;
}
.hide{display:none;}
.nav_item{
  display: inline-block;
  padding: 0 10px;
  margin: 0 5px;
  line-height: 1.4rem;
}
.nav .cur{
  background: #ff5777;
  border-radius: 10px;
  color: #fff;
}
.banner{
  background: #fff;
  margin-top:2rem;
}
.swiper {
  height: 8.8rem;
  width: 100%;
}
.swiper image {
  height: 100%;
  width: 100%;
}
.ad{

  background: #fff;
}
.ad .ad_nav{

  width: 100%;
  margin-bottom: 16rpx;
}

.ad .ad_nav image{
  width: 100%;
  height:auto;
}
.ad_line{
  width:100%;

}
.ad_line_img{
  margin-bottom:10rpx;
}
.ad_line_img image{
  width:100%;
  height:100%;
}
.goods{
  overflow: hidden;
}
#quick-entrance-new-2 {
    width: 100%;
    display: block;
    position: relative;
    background-color: #fff;
    padding: 9rpx 9rpx 8rpx;
}
.qe-wrapper-2 {
    position: relative;
    width: 100%;
    background-color: #fff;
    padding: 7px 0 4px;
    white-space: nowrap;
    -webkit-overflow-scrolling: touch;
    overflow: hidden;
}
.qe-wrapper-2 .li {
    width: 20%;
    float: left;
    display: block;
    margin-bottom: 10rpx;
}
.qe-wrapper-2 .li:nth-child(1) {
    margin-left: 0;
}
.qe-wrapper-2 .li .quick-entrance-cube {
    display: block;
    padding: 0;
    width: 100%;
    height: inherit;
    text-align: center;
    color: #fff;
    position: relative;
}
.qe-wrapper-2 .li .quick-entrance-cube image {
    width: 78rpx;
    height: 78rpx;
}
.qe-wrapper-2 .li .quick-entrance-cube .cube-text {
    position: relative;
    width: 100%;
    height: 32rpx;
    line-height: 32rpx;
    color: #777;
    margin-top: 5rpx;
    font-size: 24rpx;
}

.span{display: inline;}
.guess_box{
  width:100%;
}
.floor_title {
      padding: 30rpx 0 20rpx 0;
    text-align: center;
    line-height: 1;
}
.title_bg{
      display: inline-block;
    background-position: center center;
    background-repeat: no-repeat;
    line-height: 1;
    margin-bottom: 8rpx;
    text-align: center;
    margin-right: 4rpx;
    font-size: 30rpx;
    color: #333;
    width: 313rpx;
    background-image: url(https://mall.shiziyu888.com/Common/images/upload_ifrwkyzymu2damdgg4zdambqhayde_313x10.png);
    background-size: 313rpx auto;
}

.bottom_view{
height:50px;
}

.clearfix {
    zoom: 1;
}
.item-wrap {
    float: left;
    width: 50%;
    overflow: hidden;
}
.bot_box {
    margin: 0 18rpx;
}
.iwf {
    display: block;
    position: relative;
    width: 96%;
    margin: 0 0 4% 2%;
    background: #fff;
}
.MCUBE_MOD_ID_116891 .iwf .img-wrap {
    display: block;
    height: 0;
    width: 100%;
    position: relative;
    overflow: hidden;
    background: url(data:image/png;base64,) no-repeat center center;
}

.iwf .img-wrap image {
    width: 100%;
    -webkit-animation: fadeIn .4s ease both 0s;
    animation: fadeIn .4s ease both 0s;
}

.iwf .title {
    display: block;
    width: 100%;
    font-size: 22rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    text-align: center;
    overflow: hidden;
    height: 52rpx;
    line-height:52rpx;
    color: #666;
}
.clearfix:after, .clearfix:before {
    content: "";
    display: table;
}
 .pt_icon {
    float: left;
    width: 60rpx;
    height: 28rpx;
    overflow: hidden;
    margin-top: 10rpx;
    background: url(https://mall.shiziyu888.com/Common/images/170822_2f30lg5070dje7fa9ic4ff2bkekl6_52x28.png) no-repeat;
    background-size: auto 100%;
}
.fl {
    float: left;
}
.p_price {
    max-width: 45%;
    margin-top: 2rpx;
    font-size: 30rpx;
    font-weight: 700;
    color: #333;
    overflow: hidden;
}
.fr {
    float: right;
}
 .p_feed {
    max-width: 45%;
    height: 28rpx;
    line-height: 30rpx;
    margin-top: 12rpx;
    padding-right: 30rpx;
    background: url(data:image/png;base64,) center right no-repeat;
    background-size: auto 24rpx;
    font-size: 24rpx;
    color: #999;
    overflow: hidden;
}
 
 .guess_goods:after ,.clearfix:after{
    clear: both;
    content: ".";
    display: block;
    visibility: hidden;
    height: 0;
}
.guess_box{
  margin-bottom: 10rpx;
}