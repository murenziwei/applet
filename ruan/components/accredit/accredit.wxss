
.warranty{
  position:fixed;
  top:0;
  left:0;
  right:0;
  bottom:0;
  width:100%;
  height:100%;
  z-index:999;
  background-color:rgba(0,0,0,0.3);
}
.warn-dialog{
  width:90%;
  margin:0% auto 15px;
  background-color:#fff;
  box-shadow: 0 0 3px rgba(0,0,0,0.3);
  border-radius:4px;
}

.warn-footer{
  display:flex;
  justify-content:flex-end;
}
.btn-cancel{
    color: #fff;
    background-color: #d9534f;
    border-color: #d43f3a;
}
.btn-ok{
    color: #fff;
    background-color: #5cb85c;
    border-color: #4cae4c;
}
.warn-footer>button{
  font-size:30rpx;
  margin:0;
  flex:1;
  border-radius:0;
}
.warn-title{
  padding:30rpx;
  text-align:center;
  font-size:30rpx;
}
.warn-content{
  padding:0 0 25rpx 0;
  font-size:25rpx;
  opacity:0.7;
  text-align:center;
}